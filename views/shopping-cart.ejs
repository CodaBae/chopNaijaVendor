 .cart-wrapper {
     position: initial; 
    font-family: 'Montserrat',sans-serif;
    font-weight: 300;
    letter-spacing: -0.02em;
    color: #262626;
    top: 0;
    right: 0;
    bottom: 0;
    max-width: 100%;
    min-width: 100%;
    overflow: hidden;
    z-index: 10000;
    padding-left: 2.5em;
    width: 100%;
    height: 100%;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    -webkit-transition: all .5s;
    -moz-transition: all .5s;
    -ms-transition: all .5s;
    -o-transition: all .5s;
    transition: all .5s;
}
.cart-panel {
    position: initial;
    background-color: #f5f5f5;
    width: 100%;
    height: 100%;
}

.cart-sections>.cart-section {
    width: 50%;
    height: auto;
    display: table-cell;
    position: relative;
}
.nano>.nano-content {
    position: absolute;
    overflow: scroll;
    overflow-x: hidden;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    right: 20px;
}

.cart-sections {
    width: 200%;
    height: 55%;
    display: table;
    position: absolute;
    left: 0;
    -webkit-transition: all .25s;
    -moz-transition: all .25s;
    -ms-transition: all .25s;
    -o-transition: all .25s;
    transition: all .25s;
}

  header .user-basket .cart-img a {
    display: inline-block;
    width: 40%;
    padding: 0px;
}

.cart-sections {
    width: 200%;
    height: 55%;
    display: flex;
    position: absolute;
    left: 0;
    -webkit-transition: all .25s;
    -moz-transition: all .25s;
    -ms-transition: all .25s;
    -o-transition: all .25s;
    transition: all .25s;
}
