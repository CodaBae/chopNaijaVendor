<!DOCTYPE html>
<html lang="en">
  <head>
    <!-- Required meta tags-->
    <meta charset="UTF-8" />
    <meta
      name="viewport"
      content="width=device-width, initial-scale=1, shrink-to-fit=no"
    />
    <meta name="description" content="au theme template" />
    <meta name="author" content="Hau Nguyen" />
    <meta name="keywords" content="au theme template" />

    <!-- Title Page-->
    <title>Chop Naija Vendors Dashboard</title>

    <!-- Fontfaces CSS-->
    <link href="../assets/dash/css/font-face.css" rel="stylesheet" media="all" />
    <link
      href="../assets/dash/vendor/font-awesome-4.7/css/font-awesome.min.css"
      rel="stylesheet"
      media="all"
    />
    <link
      href="../assets/dash/vendor/font-awesome-5/css/fontawesome-all.min.css"
      rel="stylesheet"
      media="all"
    />
    <link
      href="../assets/dash/vendor/mdi-font/css/material-design-iconic-font.min.css"
      rel="stylesheet"
      media="all"
    />

    <!-- Bootstrap CSS-->
    <link
      href="../assets/dash/vendor/bootstrap-4.1/bootstrap.min.css"
      rel="stylesheet"
      media="all"
    />

    <!-- Vendor CSS-->
    <link
      href="../assets/dash/vendor/animsition/animsition.min.css"
      rel="stylesheet"
      media="all"
    />
    <link
      href="../assets/dash/vendor/bootstrap-progressbar/bootstrap-progressbar-3.3.4.min.css"
      rel="stylesheet"
      media="all"
    />
    <link href="../assets/dash/vendor/wow/animate.css" rel="stylesheet" media="all" />
    <link
      href="../assets/dash/vendor/css-hamburgers/hamburgers.min.css"
      rel="stylesheet"
      media="all"
    />
    <link href="../assets/dash/vendor/slick/slick.css" rel="stylesheet" media="all" />
    <link
      href="../assets/dash/vendor/select2/select2.min.css"
      rel="stylesheet"
      media="all"
    />
    <link
      href="../assets/dash/vendor/perfect-scrollbar/perfect-scrollbar.css"
      rel="stylesheet"
      media="all"
    />
    <link
      href="../assets/dash/vendor/vector-map/jqvmap.min.css"
      rel="stylesheet"
      media="all"
    />

    <link href="../assets/css/bootstrap.min.css" rel="stylesheet" />

    <!-- Custom CSS -->
    <link href="../assets/css/font-awesome.min.css" rel="stylesheet" type="text/css" />
    <link href="../assets/css/ionicons.min.css" rel="stylesheet" />
    <link href="../assets/css/main.css" rel="stylesheet" />
    <link href="../assets/css/style.css" rel="stylesheet" />
    <link href="../assets/css/responsive.css" rel="stylesheet" />

    <!-- JavaScripts -->
    <script src="../assets/js/modernizr.js"></script>

    <!-- Main CSS-->
    <link href="../assets/dash/css/theme.css" rel="stylesheet" media="all" />
    <style>
      .header-desktop2 {
        background: #252323;
      }

      .menu-sidebar2 .logo {
        background: #191818;
      }
      .btn {
        border: none;
        color: #fff;
        display: inline-block;
        padding: 5px 50px;
        text-transform: uppercase;
        font-weight: bold;
        font-size: 18px;
        border-radius: 0px;
        font-family: "Montserrat", sans-serif;
        line-height: 46px;
        background: red;
        letter-spacing: 1px;
        position: relative;
        z-index: 1;
      }
      .col-lg-6 {
      }

      @media (min-width: 1200px) {
        .col-lg-6 {
          width: 50%;
          max-width: 35%;

        }
      }
    </style>
  </head>

  <body class="animsition">
    <div class="page-wrapper">
      <!-- MENU SIDEBAR-->
      <aside class="menu-sidebar2">
        <div class="logo">
          <a href="/">
            <img src="../assets/images/logo-1.png" alt="Cool Admin" />
          </a>
        </div>
        <div class="menu-sidebar2__content js-scrollbar1">
          <div class="account2">
            <div class="image img-cir img-120">
              <img
                src="https://www.seekpng.com/png/detail/428-4287240_no-avatar-user-circle-icon-png.png"
                alt="user"
              />
            </div>
            <h4 class="name"><%=user.first_name%> <%=user.last_name%></h4>
                        <h4 class="name"> <%=user.brand_name%></h4>

          </div>
          <nav class="navbar-sidebar2">
            <ul class="list-unstyled navbar__list">
              <li class="active has-sub">
                <a class="js-arrow" href="/vendor">
                  <i class="fas fa-tachometer-alt"></i>Dashboard
                </a>
              </li>

              <li>
                <a href="/add_product">
                  <i class="fas fa-shopping-basket"></i>Add Products</a
                >
              </li>

              
            </ul>
          </nav>
        </div>
      </aside>
      <!-- END MENU SIDEBAR-->

      <!-- PAGE CONTAINER-->
      <div class="page-container2">
        <!-- HEADER DESKTOP-->
        <header class="header-desktop2">
          <div class="section__content section__content--p30">
            <div class="container-fluid">
              <div class="header-wrap2">
                <div class="logo d-block d-lg-none">
                  <a href="/">
                    <img src="../assets/images/logo-1.png" alt="chop naija" />
                  </a>
                </div>
                <div class="header-button2">
                  <div class="header-button-item js-item-menu">
                    <div class="search-dropdown js-dropdown">
                      <form action="">
                        <input
                          class="au-input au-input--full au-input--h65"
                          type="text"
                          placeholder="Search for datas &amp; reports..."
                        />
                        <span class="search-dropdown__icon">
                          <i class="zmdi zmdi-search"></i>
                        </span>
                      </form>
                    </div>
                  </div>
                  <div class="header-button-item has-noti js-item-menu">
                    <div class="notifi-dropdown js-dropdown">
                      <div class="notifi__title">
                        <p>You have 3 Notifications</p>
                      </div>
                      <div class="notifi__item">
                        <div class="bg-c1 img-cir img-40">
                          <i class="zmdi zmdi-email-open"></i>
                        </div>
                        <div class="content">
                          <p>You got a email notification</p>
                          <span class="date">April 12, 2018 06:50</span>
                        </div>
                      </div>
                      <div class="notifi__item">
                        <div class="bg-c2 img-cir img-40">
                          <i class="zmdi zmdi-account-box"></i>
                        </div>
                        <div class="content">
                          <p>Your account has been blocked</p>
                          <span class="date">April 12, 2018 06:50</span>
                        </div>
                      </div>
                      <div class="notifi__item">
                        <div class="bg-c3 img-cir img-40">
                          <i class="zmdi zmdi-file-text"></i>
                        </div>
                        <div class="content">
                          <p>You got a new file</p>
                          <span class="date">April 12, 2018 06:50</span>
                        </div>
                      </div>
                      <div class="notifi__footer">
                        <a href="/">All notifications</a>
                      </div>
                    </div>
                  </div>
                  <div class="header-button-item mr-0 js-sidebar-btn">
                    <i class="zmdi zmdi-menu"></i>
                  </div>
                  <div
                    class="setting-menu js-right-sidebar d-none d-lg-block"
                  ></div>
                </div>
              </div>
            </div>
          </div>
        </header>
        <aside class="menu-sidebar2 js-right-sidebar d-block d-lg-none">
          <div class="logo">
            <a href="/">
              <img src="../assets/images/logo-1.png" alt="Cool Admin" />
            </a>
          </div>
          <div class="menu-sidebar2__content js-scrollbar2">
            <div class="account2">
              <div class="image img-cir img-120">
                <img
                  src="https://www.seekpng.com/png/detail/428-4287240_no-avatar-user-circle-icon-png.png"
                  alt="user"
                />
              </div>
              <h4 class="name"><%=user.first_name%> <%=user.last_name%></h4>
                                  <h4 class="name"><%=user.brand_name%></h4>

            </div>
            <nav class="navbar-sidebar2">
              <ul class="list-unstyled navbar__list">
                <li class="active has-sub">
                  <a class="js-arrow" href="/vendor">
                    <i class="fas fa-tachometer-alt"></i>Dashboard
                  </a>
                </li>

                <li>
                  <a href="/add_product">
                    <i class="fas fa-shopping-basket"></i>Add Products</a
                  >
                </li>

               
              </ul>
            </nav>
          </div>
        </aside>
        <!-- END HEADER DESKTOP-->

        <!-- BREADCRUMB-->
        <section class="au-breadcrumb m-t-75">
          <div class="section__content section__content--p30">
            <div class="container-fluid">
              <div class="row">
                <div class="col-md-12">
                  <div class="au-breadcrumb-content">
                    <div class="au-breadcrumb-left">
                      <span class="au-breadcrumb-span">You are here:</span>
                      <ul class="list-unstyled list-inline au-breadcrumb__list">
                        <li class="list-inline-item active">
                          <a href="/">Home</a>
                        </li>
                        <li class="list-inline-item seprate">
                          <span>/</span>
                        </li>
                        <li class="list-inline-item">Dashboard</li>
                      </ul>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </section>
        <!-- END BREADCRUMB-->

        <!-- STATISTIC-->
        <section class="statistic">
          <div class="section__content section__content--p30">
            <div class="container-fluid">
              <div class="row">
                <div class="col-md-6 col-lg-3">
                  <div class="statistic__item">
                    <h2 class="number"><%=user.productNum%></h2>
                    <span class="desc">total products</span>
                    <div class="icon">
                      <i class="zmdi zmdi-shopping-cart"></i>
                    </div>
                  </div>
                </div>

                <div class="col-md-6 col-lg-3">
                  <div class="statistic__item">
                    <h2 class="number">0</h2>
                    <span class="desc">total orders</span>
                    <div class="icon">
                      <i class="zmdi zmdi-money"></i>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </section>
        <!-- END STATISTIC-->
        <div class="container">
          <h3 style="margin: 5% 0;">All Products</h3>

          <div class="row justify-content-center">
<% if (user.products) { %>

                   <% user.products.map((p) =>{ %>
          <div class="col-lg-6 col-md-8">
              <div class="card shadow">
                <img
                  class="card-img"
                  src="<%=p.img%>"
                  alt="succulent"
                />
                <div class="card-img-overlay text-white"></div>
                <div class="card-body">
                  <h4 class="card-title"><%=p.name%></h4>

                  <p class="card-text">
                    <%=p.delivery%>
                    
                  </p>

                  <p class="card-text" style="margin: 5% 0;">
                    <small class="text-muted" style="margin: 0 3%;">
                      price : $<%=p.price%>
                    </small>
                    <small class="text-muted" style="margin: 0 3%;">
                      quantity : <%=p.quantity%>
                    </small>
                    <small class="text-muted" style="margin: 0 3%;">
                      discount : <%=p.discount%>%
                    </small>
                  </p>

                  <li class="col-md-6">
                    <button type="submit" class="btn">DELETE</button>
                  </li>
                </div>
              </div>
            </div>
            
     <% }) %>

               <% } %>
          </div>
        </div>

        <!-- END PAGE CONTAINER-->
      </div>
    </div>

    <!-- Jquery JS-->
    <script src="../assets/dash/vendor/jquery-3.2.1.min.js"></script>
    <!-- Bootstrap JS-->
    <script src="../assets/dash/vendor/bootstrap-4.1/popper.min.js"></script>
    <script src="../assets/dash/vendor/bootstrap-4.1/bootstrap.min.js"></script>
    <!-- Vendor JS       -->
    <script src="../assets/dash/vendor/slick/slick.min.js"></script>
    <script src="../assets/dash/vendor/wow/wow.min.js"></script>
    <script src="../assets/dash/vendor/animsition/animsition.min.js"></script>
    <script src="../assets/dash/vendor/bootstrap-progressbar/bootstrap-progressbar.min.js"></script>
    <script src="../assets/dash/vendor/counter-up/jquery.waypoints.min.js"></script>
    <script src="../assets/dash/vendor/counter-up/jquery.counterup.min.js"></script>
    <script src="../assets/dash/vendor/circle-progress/circle-progress.min.js"></script>
    <script src="../assets/dash/vendor/perfect-scrollbar/perfect-scrollbar.js"></script>
    <script src="../assets/dash/vendor/chartjs/Chart.bundle.min.js"></script>
    <script src="../assets/dash/vendor/select2/select2.min.js"></script>
    <script src="../assets/dash/vendor/vector-map/jquery.vmap.js"></script>
    <script src="../assets/dash/vendor/vector-map/jquery.vmap.min.js"></script>
    <script src="../assets/dash/vendor/vector-map/jquery.vmap.sampledata.js"></script>
    <script src="../assets/dash/vendor/vector-map/jquery.vmap.world.js"></script>

    <!-- Main JS-->
    <script src="../assets/dash/js/main.js"></script>
  </body>
</html>
<!-- end document-->
