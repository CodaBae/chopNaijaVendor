<!DOCTYPE html>
<html lang="en">

<head>
    <!-- Required meta tags-->
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="au theme template">
    <meta name="author" content="Hau Nguyen">
    <meta name="keywords" content="au theme template">

    <!-- Title Page-->
    <title>Chop Naija Vendors Dashboard</title>

    <!-- Fontfaces CSS-->
    <link href="../assets/dash/css/font-face.css" rel="stylesheet" media="all">
    <link href="../assets/dash/vendor/font-awesome-4.7/css/font-awesome.min.css" rel="stylesheet" media="all">
    <link href="../assets/dash/vendor/font-awesome-5/css/fontawesome-all.min.css" rel="stylesheet" media="all">
    <link href="../assets/dash/vendor/mdi-font/css/material-design-iconic-font.min.css" rel="stylesheet" media="all">

    <!-- Bootstrap CSS-->
    <link href="../assets/dash/vendor/bootstrap-4.1/bootstrap.min.css" rel="stylesheet" media="all">

    <!-- Vendor CSS-->
    <link href="../assets/dash/vendor/animsition/animsition.min.css" rel="stylesheet" media="all">
    <link href="../assets/dash/vendor/bootstrap-progressbar/bootstrap-progressbar-3.3.4.min.css" rel="stylesheet" media="all">
    <link href="../assets/dash/vendor/wow/animate.css" rel="stylesheet" media="all">
    <link href="../assets/dash/vendor/css-hamburgers/hamburgers.min.css" rel="stylesheet" media="all">
    <link href="../assets/dash/vendor/slick/slick.css" rel="stylesheet" media="all">
    <link href="../assets/dash/vendor/select2/select2.min.css" rel="stylesheet" media="all">
    <link href="../assets/dash/vendor/perfect-scrollbar/perfect-scrollbar.css" rel="stylesheet" media="all">
    <link href="../assets/dash/vendor/vector-map/jqvmap.min.css" rel="stylesheet" media="all">

    <link href="../assets/css/bootstrap.min.css" rel="stylesheet">

    <!-- Custom CSS -->
    <link href="../assets/css/font-awesome.min.css" rel="stylesheet" type="text/css">
    <link href="../assets/css/ionicons.min.css" rel="stylesheet">
    <link href="../assets/css/main.css" rel="stylesheet">
    <link href="../assets/css/style.css" rel="stylesheet">
    <link href="../assets/css/responsive.css" rel="stylesheet">

    <!-- JavaScripts -->
    <script src="../assets/js/modernizr.js"></script>

    <!-- Online Fonts -->
    <link href='https://fonts.googleapis.com/css?family=Montserrat:400,700' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Playfair+Display:400,700,900' rel='stylesheet' type='text/css'>

    <!-- Main CSS-->
    <link href="../assets/dash/css/theme.css" rel="stylesheet" media="all">
    <style>
        .header-desktop2 {
            background: #252323;

        }

        .menu-sidebar2 .logo {

            background: #191818;
        }


        .upload-btn-wrapper {
            position: relative;
            overflow: hidden;
            display: inline-block;
        }



        .upload-btn-wrapper input[type=file] {
            font-size: 100px;
            position: absolute;
            left: 0;
            top: 0;
            opacity: 0;
        }

        .btn {
            border: none;
            color: #fff;
            display: inline-block;
            padding: 5px 50px;
            text-transform: uppercase;
            font-weight: bold;
            font-size: 18px;
            border-radius: 0px;
            font-family: 'Montserrat', sans-serif;
            line-height: 46px;
            background: #80c337;
            letter-spacing: 1px;
            position: relative;
            z-index: 1;
        }
        .fade {
    opacity: 5;
    }
    </style>
</head>

<body class="animsition">
    <div class="page-wrapper">
        <!-- MENU SIDEBAR-->
        <aside class="menu-sidebar2">
            <div class="logo">
                <a href="../assets/dash/#">
                    <img src="../assets/images/logo-1.png" alt="Cool Admin" />
                </a>
            </div>
            <div class="menu-sidebar2__content js-scrollbar1">
                <div class="account2">
                    <div class="image img-cir img-120">
                        <img src="https://www.seekpng.com/png/detail/428-4287240_no-avatar-user-circle-icon-png.png"
                            alt="user" />
                    </div>
                    <h4 class="name"><%=user.first_name%> <%=user.last_name%></h4>
                    <h4 class="name"><%=user.brand_name%></h4>

                </div>
                <nav class="navbar-sidebar2">
                    <ul class="list-unstyled navbar__list">
                        <li>
                            <a class="js-arrow" href="/vendor">
                                <i class="fas fa-tachometer-alt"></i>Dashboard

                            </a>

                        </li>

                        <li class="active has-sub">
                            <a href="/add_product">
                                <i class="fas fa-shopping-basket"></i>Add Products</a>
                        </li>



                    </ul>
                </nav>
            </div>
        </aside>
        <!-- END MENU SIDEBAR-->

        <!-- PAGE CONTAINER-->
        <div class="page-container2">
            <!-- HEADER DESKTOP-->
            <header class="header-desktop2">
                <div class="section__content section__content--p30">
                    <div class="container-fluid">
                        <div class="header-wrap2">
                            <div class="logo d-block d-lg-none">
                                <a href="../assets/dash/#">
                                    <img src="../assets/images/logo-1.png" alt="chop naija" />
                                </a>
                            </div>
                            <div class="header-button2">
                                <div class="header-button-item js-item-menu">
                                    <div class="search-dropdown js-dropdown">
                                        <form action="">
                                            <input class="au-input au-input--full au-input--h65" type="text"
                                                placeholder="Search for datas &amp; reports..." />
                                            <span class="search-dropdown__icon">
                                                <i class="zmdi zmdi-search"></i>
                                            </span>
                                        </form>
                                    </div>
                                </div>
                                <div class="header-button-item has-noti js-item-menu">
                                    <div class="notifi-dropdown js-dropdown">
                                        <div class="notifi__title">
                                            <p>You have 3 Notifications</p>
                                        </div>
                                        <div class="notifi__item">
                                            <div class="bg-c1 img-cir img-40">
                                                <i class="zmdi zmdi-email-open"></i>
                                            </div>
                                            <div class="content">
                                                <p>You got a email notification</p>
                                                <span class="date">April 12, 2018 06:50</span>
                                            </div>
                                        </div>
                                        <div class="notifi__item">
                                            <div class="bg-c2 img-cir img-40">
                                                <i class="zmdi zmdi-account-box"></i>
                                            </div>
                                            <div class="content">
                                                <p>Your account has been blocked</p>
                                                <span class="date">April 12, 2018 06:50</span>
                                            </div>
                                        </div>
                                        <div class="notifi__item">
                                            <div class="bg-c3 img-cir img-40">
                                                <i class="zmdi zmdi-file-text"></i>
                                            </div>
                                            <div class="content">
                                                <p>You got a new file</p>
                                                <span class="date">April 12, 2018 06:50</span>
                                            </div>
                                        </div>
                                        <div class="notifi__footer">
                                            <a href="../assets/dash/#">All notifications</a>
                                        </div>
                                    </div>
                                </div>
                                <div class="header-button-item mr-0 js-sidebar-btn">
                                    <i class="zmdi zmdi-menu"></i>
                                </div>
                                <div class="setting-menu js-right-sidebar d-none d-lg-block">

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </header>
            <aside class="menu-sidebar2 js-right-sidebar d-block d-lg-none">
                <div class="logo">
                    <a href="../assets/dash/#">
                        <img src="../assets/images/logo-1.png" alt="Cool Admin" />
                    </a>
                </div>
                <div class="menu-sidebar2__content js-scrollbar2">
                    <div class="account2">
                        <div class="image img-cir img-120">
                            <img src="https://www.seekpng.com/png/detail/428-4287240_no-avatar-user-circle-icon-png.png"
                                alt="user" />
                        </div>
                        <h4 class="name"><%=user.first_name%> <%=user.last_name%></h4>
                                            <h4 class="name"><%=user.brand_name%></h4>

                    </div>
                    <nav class="navbar-sidebar2">
                        <ul class="list-unstyled navbar__list">
                            <li>
                                <a class="js-arrow" href="/vendor">
                                    <i class="fas fa-tachometer-alt"></i>Dashboard

                                </a>

                            </li>

                            <li class="active has-sub">
                                <a href="/add_product">
                                    <i class="fas fa-shopping-basket"></i>Add Products</a>
                            </li>



                        </ul>
                    </nav>
                </div>
            </aside>
            <!-- END HEADER DESKTOP-->

            <!-- BREADCRUMB-->
            <section class="au-breadcrumb m-t-75">
                <div class="section__content section__content--p30">
                    <div class="container-fluid">
                        <div class="row">
                            <div class="col-md-12">
                                <div class="au-breadcrumb-content">
                                    <div class="au-breadcrumb-left">
                                        <span class="au-breadcrumb-span">You are here:</span>
                                        <ul class="list-unstyled list-inline au-breadcrumb__list">
                                            <li class="list-inline-item active">
                                                <a href="/">Home</a>
                                            </li>
                                            <li class="list-inline-item seprate">
                                                <span>/</span>
                                            </li>
                                            <li class="list-inline-item">Add Products</li>

                                        </ul>
                                    </div>

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
            <!-- END BREADCRUMB-->

            <!--======= PAGES INNER =========-->
            <section class="chart-page padding-top-100 padding-bottom-100">
                <div class="container">

                    <!-- Payments Steps -->
                    <div class="shopping-cart">

                        <!-- SHOPPING INFORMATION -->
                        <div class="cart-ship-info register">
                            <div class="row">

                                <!-- ESTIMATE SHIPPING & TAX -->
                                <div class="col-sm-12">
                                    <h6>ADD PRODUCT</h6>

                                                        <% include ./partials/messages %>

                                    <form action="/users/add_product?id=<%=user.id%>" method="POST">
                                        <ul class="row">

                                            <!-- Name -->
                                            <li class="col-md-6">
                                                <label> *NAME
                                                    <input type="text" name="name" value="" placeholder="product name">
                                                </label>
                                            </li>
                                            <!-- LAST NAME -->
                                            <li class="col-md-6">
                                                <label> *BRAND NAME
                                                    <input type="text" name="brand_name" value="<%=user.brand_name%>" placeholder="brand name">
                                                </label>
                                            </li>

                                            <li class="col-md-6">
                                                <label> *PRICE
                                                    <input type="number" min='1' max='100000000' name="price" value=""
                                                        placeholder="price of product">
                                                </label>
                                            </li>


                                            <li class="col-md-6">
                                                <label> *QUANTITY
                                                    <input type="number" min='1' max='100' name="quantity" value=""
                                                        placeholder="quantity of product">
                                                </label>
                                            </li>

                                            <li class="col-md-6">
                                                <label> *DISCOUNT
                                                    <input type="number" min='0' max='100' name="discount" value=""
                                                        placeholder="discount percentage">
                                                </label>
                                            </li>
                                            <!-- PHONE -->

                                            <!-- EMAIL ADDRESS -->
                                            <li class="col-md-6">
                                                <label> *DELIVERY INFORMATION(a detailed information on how you sell
                                                    this products)
                                                    <input type="text" name="delivery" value=""
                                                        placeholder="delivery data">
                                                </label>
                                            </li>


                                            <div class="upload-btn-wrapper" style='margin-top:5%; width: 100%;'>

                                                <button class="btn" style="width: 100%;">upload your product image
                                                    <input type="file" accept="image/png, image/jpeg, image/jpg"
                                                        value="" name="myfile" id='F' class='change' />

                                                </button>
                                                <input type="hidden" value="" name="img" id='HF' />


                                                <label for="create_product_image" class="product_image_label"
                                                    style="outline:none; color:#6a1b8b; background:none; text-align:center; padding: 15px 0px 8px">Click
                                                    to choose a file
                                            </div>

                                            <!-- PHONE -->
                                            <li class="col-md-6">
                                                <button type="submit" class="btn">ADD NOW</button>
                                            </li>
                                        </ul>
                                    </form>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>

            <section>
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="copyright">
                            </div>
                        </div>
                    </div>
                </div>
            </section>
            <!-- END PAGE CONTAINER-->
        </div>

    </div>

    <!-- Jquery JS-->
    <script src="../assets/dash/vendor/jquery-3.2.1.min.js"></script>
    <!-- Bootstrap JS-->
    <script src="../assets/dash/vendor/bootstrap-4.1/popper.min.js"></script>
    <script src="../assets/dash/vendor/bootstrap-4.1/bootstrap.min.js"></script>
    <!-- Vendor JS       -->
    <script src="../assets/dash/vendor/slick/slick.min.js">
    </script>
    <script src="../assets/dash/vendor/wow/wow.min.js"></script>
    <script src="../assets/dash/vendor/animsition/animsition.min.js"></script>
    <script src="../assets/dash/vendor/bootstrap-progressbar/bootstrap-progressbar.min.js">
    </script>
    <script src="../assets/dash/vendor/counter-up/jquery.waypoints.min.js"></script>
    <script src="../assets/dash/vendor/counter-up/jquery.counterup.min.js">
    </script>
    <script src="../assets/dash/vendor/circle-progress/circle-progress.min.js"></script>
    <script src="../assets/dash/vendor/perfect-scrollbar/perfect-scrollbar.js"></script>
    <script src="../assets/dash/vendor/chartjs/Chart.bundle.min.js"></script>
    <script src="../assets/dash/vendor/select2/select2.min.js">
    </script>
    <script src="../assets/dash/vendor/vector-map/jquery.vmap.js"></script>
    <script src="../assets/dash/vendor/vector-map/jquery.vmap.min.js"></script>
    <script src="../assets/dash/vendor/vector-map/jquery.vmap.sampledata.js"></script>
    <script src="../assets/dash/vendor/vector-map/jquery.vmap.world.js"></script>

    <!-- Main JS-->
    <script src="../assets/dash/js/main.js"></script>
    <script>

        $(".change").change(function () {
            $(".product_image_label").html("Image uploaded successfully")
            productImageUpload()
        });

        function productImageUpload() {
            var oFReader = new FileReader();
            oFReader.readAsDataURL(document.getElementById("F").files[0]);

            oFReader.onload = function (oFREvent) {
                document.getElementById("HF").value = oFREvent.target.result;

                // $('#imageP').attr('src', oFREvent.target.result)
                // i = $('#imageP').attr('src')

            };
        }

    </script>

</body>

</html>
<!-- end document-->